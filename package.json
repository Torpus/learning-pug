{
  "name": "learning-pug",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./app.js",
    "lint": "eslint .",
    "audit": "npm audit --audit-level=moderate",
    "start": "node ./bin/www",
    "test": "nyc ava -v",
    "codecov": "codecov -f coverage/coverage-final.json"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "ava": "^3.12.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "semantic-release": "^17.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
